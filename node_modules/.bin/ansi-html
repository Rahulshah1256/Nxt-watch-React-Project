#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/ansi-html/bin/ansi-html/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/ansi-html/bin/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/ansi-html/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules:/home/nxtwave/reactjs/coding-practices/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ansi-html/bin/ansi-html" "$@"
else 
  exec node  "$basedir/../ansi-html/bin/ansi-html" "$@"
fi
